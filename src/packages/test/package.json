{
    "name": "@springtype/test",
    "version": "1.0.0-alpha.1.38",
    "publishConfig": {
        "access": "public"
    },
    "description": "Mocha/Chai test adapters for SpringType.",
    "dependencies": {
        "@springtype/core": "../core",
        "@springtype/ssr": "../ssr",
        "@types/node": "^11.10.5",
        "reflect-metadata": "^0.1.12",
        "tslib": "^1.9.3"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/springtype-org/springtype.git"
    },
    "scripts": {
        "bootstrap": "npm run bootstrap:test --prefix ../../",
        "clean:all": "npm run clean:test --prefix ../../",
        "clean": "npm run clean:test --prefix ../../",
        "build": "npm run build:test --prefix ../../",
        "watch": "npm run watch:test --prefix ../../",
        "release": "npm run release:test --prefix ../../",
        "test": "mocha -r ts-node/register test/**/*.test.ts"
    },
    "author": "Aron Homberg",
    "license": "MIT",
    "browserslist": [
        "last 1 Chrome version"
    ],
    "devDependencies": {
        "@tbranyen/jsdom": "^13.0.0",
        "@types/chai": "^4.1.7",
        "@types/mocha": "^5.2.6",
        "chai": "^4.2.0",
        "mocha": "^6.1.3",
        "ts-node": "^7.0.1",
        "typescript": "^3.3.4000"
    }
}
