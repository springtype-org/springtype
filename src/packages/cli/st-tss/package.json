{
    "name": "st-tss",
    "version": "1.0.0-alpha.1.41",
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "springtype CSS/SCSS/Sass/Less to TSS transformer",
        "st-tss"
    ],
    "description": "CLI tool for bulk-transforming CSS, SCSS, Sass and Less into TSS.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/springtype-org/springtype.git"
    },
    "scripts": {
        "bootstrap": "npm run bootstrap:cli/st-tss --prefix ../../../",
        "clean:all": "npm run clean:all:cli/st-tss --prefix ../../../",
        "clean": "npm run clean:cli/st-tss --prefix ../../../",
        "watch": "npm run watch:cli/st-tss --prefix ../../../",
        "build": "npm run build:cli/st-tss --prefix ../../../",
        "release": "npm run release:cli/st-tss --prefix ../../../",
        "start": "npm run start:cli/st-tss --prefix ../../../",
        "test": "npm run test:cli/st-tss --prefix ../../../"
    },
    "engines": {
        "node": ">=10"
    },
    "bundledDependencies": [
        "chalk"
    ],
    "bin": {
        "st-tss": "./dist/cli.js"
    },
    "local:main": "src/index.ts",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "Aron Homberg, Michael Mannseicher",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/springtype-org/springtype/issues"
    },
    "homepage": "https://springtype.org",
    "dependencies": {
        "@csstools/postcss-sass": "^4.0.0",
        "chalk": "^2.4.2",
        "glob": "^7.1.4",
        "postcss": "^7.0.17",
        "postcss-less": "^3.1.4",
        "postcss-scss": "^2.0.0",
        "source-map": "^0.7.3"
    },
    "devDependencies": {
        "@types/node": "^11.13.7",
        "node": "^12.6.0",
        "typescript": "^3.3.4000"
    }
}