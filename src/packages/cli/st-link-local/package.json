{
    "name": "st-link-local",
    "version": "1.0.0-alpha.1.49",
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "yarn",
        "npm",
        "node",
        "link",
        "sync",
        "local",
        "dependency"
    ],
    "description": "Watches locally file:-referenced dependencies and syncs them into ./node_modules using yarn",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/springtype-org/springtype.git"
    },
    "scripts": {
        "bootstrap": "npm run bootstrap:cli/st-link-local --prefix ../../../",
        "clean:all": "npm run clean:all:cli/st-link-local --prefix ../../../",
        "clean": "npm run clean:cli/st-link-local --prefix ../../../",
        "watch": "npm run watch:cli/st-link-local--prefix ../../../",
        "build": "npm run build:cli/st-link-local --prefix ../../../",
        "release": "npm run release:cli/st-link-local --prefix ../../../",
        "start": "npm run start:cli/st-link-local --prefix ../../../",
        "test": "npm run test:cli/st-link-local --prefix ../../../"
    },
    "engines": {
        "node": ">=10"
    },
    "bundledDependencies": [
        "chalk",
        "chokidar",
        "st-cp",
        "st-rm-rf",
        "glob"
    ],
    "bin": {
        "st-link-local": "./dist/cli.js"
    },
    "local:main": "src/index.ts",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "Aron Homberg",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/springtype-org/springtype/issues"
    },
    "homepage": "https://springtype.org",
    "stLinkLocalDependencies": {
        "@springtype/core": "file:../../core/dist"
    },
    "dependencies": {
        "chalk": "^2.4.2",
        "chokidar": "^3.0.2",
        "glob": "^7.1.4",
        "st-cp": "^1.0.0-alpha.1.41",
        "st-rm-rf": "^1.0.0-alpha.1.39"
    },
    "devDependencies": {
        "@types/node": "^11.13.7",
        "node": "^12.6.0",
        "typescript": "^3.3.4000"
    }
}