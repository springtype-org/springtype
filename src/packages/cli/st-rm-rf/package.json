{
    "name": "st-rm-rf",
    "version": "1.0.0-alpha.1.38",
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "springtype rm -rf",
        "st-rm-rf"
    ],
    "description": "CLI tool for platform independent removing of files and folders.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/springtype-org/springtype.git"
    },
    "scripts": {
        "bootstrap": "npm run bootstrap:cli/st-rm-rf --prefix ../../../",
        "clean:all": "npm run clean:all:cli/st-rm-rf --prefix ../../../",
        "clean": "npm run clean:cli/st-rm-rf --prefix ../../../",
        "watch": "npm run watch:cli/st-rm-rf --prefix ../../../",
        "build": "npm run build:cli/st-rm-rf --prefix ../../../",
        "release": "npm run release:cli/st-rm-rf --prefix ../../../",
        "start": "npm run start:cli/st-rm-rf --prefix ../../../"
    },
    "engines": {
        "node": ">=10"
    },
    "bundledDependencies": [
        "chalk"
    ],
    "bin": {
        "st-rm-rf": "./dist/index.js"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "Aron Homberg, Michael Mannseicher",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/springtype-org/springtype/issues"
    },
    "homepage": "https://springtype.org",
    "dependencies": {
        "chalk": "^2.4.2"
    },
    "devDependencies": {
        "@types/node": "^11.13.7",
        "node": "^12.6.0",
        "typescript": "^3.3.4000"
    }
}
