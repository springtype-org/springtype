{
    "name": "st-create",
    "version": "1.0.0-alpha.1.50",
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "springtype create",
        "st-create",
        "scaffold"
    ],
    "description": "Shortcut for the CLI command: springtype create",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/springtype-org/springtype.git"
    },
    "scripts": {
        "bootstrap": "npm run bootstrap:cli/st-create --prefix ../../../",
        "clean:all": "npm run clean:all:cli/st-create --prefix ../../../",
        "clean": "npm run clean:cli/st-create --prefix ../../../",
        "watch": "npm run watch:cli/st-create --prefix ../../../",
        "build": "npm run build:cli/st-create --prefix ../../../",
        "release": "npm run release:cli/st-create --prefix ../../../",
        "start": "npm run start:cli/st-create --prefix ../../../",
        "test": "npm run test:cli/st-create --prefix ../../../"
    },
    "engines": {
        "node": ">=10"
    },
    "bundledDependencies": [
        "springtype"
    ],
    "bin": {
        "st-create": "./dist/cli.js"
    },
    "local:main": "src/index.ts",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "Aron Homberg, Michael Mannseicher",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/springtype-org/springtype/issues"
    },
    "homepage": "https://springtype.org",
    "dependencies": {
        "@springtype/core": "../../core/dist"
    },
    "devDependencies": {
        "@types/node": "^11.13.7",
        "node": "^12.6.0",
        "typescript": "^3.3.4000"
    }
}