{
    "name": "st-ssr",
    "version": "1.0.0-alpha.1.40",
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "springtype server-side-rendering (SSR) library and CLI",
        "st-ssr"
    ],
    "description": "CLI tool and library for high-performance headless chrome based server-side-rendering.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/springtype-org/springtype.git"
    },
    "scripts": {
        "bootstrap": "npm run bootstrap:cli/st-ssr --prefix ../../../",
        "clean:all": "npm run clean:all:cli/st-ssr --prefix ../../../",
        "clean": "npm run clean:cli/st-ssr --prefix ../../../",
        "watch": "npm run watch:cli/st-ssr --prefix ../../../",
        "build": "npm run build:cli/st-ssr --prefix ../../../",
        "release": "npm run release:cli/st-ssr --prefix ../../../",
        "start": "npm run start:cli/st-ssr --prefix ../../../",
        "test": "npm run test:cli/st-ssr --prefix ../../../",
        "postinstall": "npm install puppeteer"
    },
    "engines": {
        "node": ">=10"
    },
    "bundledDependencies": [
        "chalk"
    ],
    "bin": {
        "st-ssr": "./dist/cli.js"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "Aron Homberg, Michael Mannseicher",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/springtype-org/springtype/issues"
    },
    "homepage": "https://springtype.org",
    "dependencies": {
        "chalk": "^2.4.2",
        "puppeteer": "^1.19.0"
    },
    "devDependencies": {
        "@types/node": "^11.13.7",
        "node": "^12.6.0",
        "typescript": "^3.3.4000"
    }
}